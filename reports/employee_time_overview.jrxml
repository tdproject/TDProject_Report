						<font fontName="DejaVu Sans" isBold="true" isItalic="true"/>
					</textElement>
					<textFieldExpression><![CDATA[( $V{EMPLOYEE_SUM} > 0 ) ? " (" + ( $V{EMPLOYEE_SUM_BILLABLE} / $V{EMPLOYEE_SUM} ).setScale(2, BigDecimal.ROUND_UP) + ")" : " (0)" ]]></textFieldExpression>
				</textField>
